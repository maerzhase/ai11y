	},
	"dependencies": {
		"turbo": "2.7.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"typedoc": "^0.28.16"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=18"
	}
}
