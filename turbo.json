{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["^build"]
		},
		"watch": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["^build"]
		}
	}
}
