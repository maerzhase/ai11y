  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch"
  },
  "devDependencies": {
    "tsdown": "0.15.5",
    "typescript": "5.9.2"
  }
}
